{
  "name": "CleanexeServer",
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "nodemon --inspect-brk -r",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && tsc",
    "start:prod": "node dist/main.js",
    "start:hmr": "node dist/server",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "webpack": "webpack --config webpack.config.js",
    "stop": "npx kill-port 9001; npx kill-port 9002; npx kill-port 9229; npx kill-port 3000; kill $(pidof nodemon); kill $(pidof node)",
    "typeorm": "ts-node ./node_modules/.bin/typeorm"
  },
  "dependencies": {
    "@nestjs/common": "^5.6.2",
    "@nestjs/core": "^5.6.2",
    "@nestjs/graphql": "^5.5.1",
    "@nestjs/microservices": "^5.6.2",
    "@nestjs/typeorm": "^5.2.2",
    "@nestjs/websockets": "^5.6.2",
    "apollo-link-ws": "^1.0.12",
    "apollo-server": "^2.3.1",
    "apollo-server-express": "^2.3.1",
    "global": "^4.3.2",
    "graphql": "^14.1.1",
    "graphql-subscriptions": "^1.0.0",
    "graphql-tools": "^4.0.3",
    "graphql-type-json": "^0.2.1",
    "kill-port": "^1.3.2",
    "mysql": "^2.16.0",
    "pm2": "^3.2.9",
    "readline": "^1.3.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.3.3",
    "sql-template-strings": "^2.2.2",
    "subscriptions-transport-ws": "^0.9.15",
    "typeorm": "^0.2.12",
    "typescript": "^3.2.4",
    "ws": "^6.1.2"
  },
  "devDependencies": {
    "@nestjs/testing": "^5.6.2",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.13",
    "@types/node": "^10.12.18",
    "@types/supertest": "^2.0.7",
    "jest": "^23.6.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.16.0",
    "rimraf": "^2.6.3",
    "supertest": "^3.4.1",
    "ts-jest": "^23.10.5",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.0.1",
    "tsconfig-paths": "^3.7.0",
    "tslint": "^5.12.1",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-node-externals": "^1.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "detectOpenHandles": true,
    "detectLeaks": true,
    "verbose": true
  }
}
